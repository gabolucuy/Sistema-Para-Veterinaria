<style type="text/css">

  h1{
    color:white;
   }

   thead{
    color:white;
   }

    td{
    background-color: white;
   }

</style>
<p id="notice"><%= notice %></p>

<h1>Listing Atencions</h1>
<div class="row">
<div class="col-md-6 col-md-offset-3">
  <table  class="table table-striped">
    <thead>
      <tr>
        <th>Fecha</th>
        <th>Cliente</th>
        <th>Mascota nombre</th>
        <th>Anamnesis</th>
        <th>Temperatura</th>
        <th>Mucosas</th>
        <th>Palpacion</th>
        <th>Diagnostico</th>
        <th>Tratamiento</th>
        <th>Veterinario</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @atencions.each do |atencion| %>
        <tr>
          <td><%= atencion.fecha %></td>
          <td><%= atencion.cliente_id %></td>
          <td><%= atencion.mascota_nombre %></td>
          <td><%= atencion.anamnesis %></td>
          <td><%= atencion.temperatura %></td>
          <td><%= atencion.mucosas %></td>
          <td><%= atencion.palpacion %></td>
          <td><%= atencion.diagnostico %></td>
          <td><%= atencion.tratamiento %></td>
          <td><%= atencion.Idveterinario %></td>
          <td><%= link_to 'Ver', atencion, class: "btn btn-info" %></td>
          <td><%= link_to 'Editar', edit_atencion_path(atencion) , class: "btn btn-warning"%></td>
          <td><%= link_to 'Eliminar', atencion, method: :delete, data: { confirm: 'Esta seguro?' } , class: "btn btn-danger"%></td>
        </tr>
        <% end %>
      </tbody>
    </table>
</div>
</div>
<br>

<%= link_to 'New Atencion', new_atencion_path , class: "btn btn-success" %>
<a  class="btn btn-primary" href="veterinaria">Home</a>
